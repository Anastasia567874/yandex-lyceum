from functools import total_ordering


@total_ordering
class Point:
    def __init__(self, name, x, y):
        self.name = name
        self.x = x
        self.y = y

    def __invert__(self):
        return Point(self.name, self.y, self.x)

    def get_x(self):
        return self.x

    def get_y(self):
        return self.y

    def get_coords(self):
        return (self.x, self.y)

    def __str__(self):
        return f'{self.name}({self.x}, {self.y})'

    def __repr__(self):
        return f"Point('{self.name}', {self.x}, {self.y})"

    def __lt__(self, other):
        if self.name < other.name:
            return True
        if self.name == other.name:
            if self.x < other.x:
                return True
            if self.x == other.x:
                if self.y < other.y:
                    return True
        return False

    def __eq__(self, other):
        if self.name == other.name and self.x == other.x and self.y == other.y:
            return True
        return False

    def __hash__(self):
        return hash(self.name) + hash(self.x) + hash(self.y)