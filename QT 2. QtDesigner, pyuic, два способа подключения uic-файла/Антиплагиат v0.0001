from PyQt5.QtWidgets import QApplication, QMainWindow
import sys


class MyWidget(QMainWindow, Ui_MainWindow):
    def __init__(self):
        super().__init__()
        self.setupUi(self)
        self.pushButton.clicked.connect(self.print)
        self.events = []

    def print(self):
        ev = self.spinBox.text()
        d = self.textEdit.toPlainText().split('\n')
        v = self.textEdit_2.toPlainText().split('\n')
        k, p = 0, 0
        for i in range(min(len(d), len(v))):
            if d[i] == v[i]:
                k += 1
            p += 1
        k = k / p * 100
        if k < int(ev):
            self.label_4.setStyleSheet('background-color: #00aa00')
        else:
            self.label_4.setStyleSheet('background-color: #aa0000')
        self.label_4.setText(f'Код похож на {k}%')


if __name__ == '__main__':
    app = QApplication(sys.argv)
    ex = MyWidget()
    ex.show()
    sys.exit(app.exec_())
